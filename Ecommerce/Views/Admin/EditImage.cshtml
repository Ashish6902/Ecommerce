@model Ecommerce.Models.AdminUiImage.ImagesUI

@{
    ViewBag.Title = "EditImage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>EditImage</h2>

@using (Html.BeginForm("EditImage", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        <label class="formFile" for="imageFile">Select an image:</label>
        <input class="form-control"type="file" id="imageFile" name="imageFile" />
    </div>
    <br />
    <div>
        <input  class="btn btn-primary" type="submit" value="Upload Image" class="btn-primary" />
    </div>
}
<br />  
<div>
    @Html.ActionLink("Back to List", "ImagesOfUI","Admin", new { @class = "nav-button" })
</div>
